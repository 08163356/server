				"outbox": {"string": "secret", "file": ""},
				"session": {"string": "secret", "file": ""}
			},
			"token": {
				"enable": {
					"browser": false,
					"request": {
						"inbox": false,
						"outbox": false
					}
				},
				"inbox": {
					"header": "Authorization",
					"prefix": "Bearer "
				},
				"outbox": {
					"header": "Authorization",
					"prefix": "Bearer ",
					"algorithm": "HS256",
					"expires": "5m"
				},
				"session": {
					"algorithm": "HS256",
					"expires": "5m"
				}
			},
			"plugins": {
				"path": "../../../sdkjs-plugins",
				"url": "../../../../sdkjs-plugins"
			}
		}
